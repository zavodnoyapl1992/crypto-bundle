{
    "name": "zavodnoyapl/crypto-bundle",
    "description": "Symfony 2/3/4 bundle for encrypt data with openssl library",
    "keywords": ["encrypt", "decrypt", "doctrine", "aes256"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya Kovalenko",
            "email": "agentsib@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AgentSIB\\CryptoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AgentSIB\\CryptoBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4",
        "ext-openssl": "*",
        "symfony/framework-bundle": ">=2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.36"
    }
}
